{
  "manifest_version": 3,
  "name": "Notelee",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Notelee"
  },
  "content_scripts": [
    
    {
      "js": [
        "accountManagement.js",
        "notes.js",
        "folders.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "background":{
    "service-worker":"firebase-main.js",
    "type": "module"
    
  }
}
